    <body>
        <div class="container">
            
            <div class="heading">
                <h1><a href="#">WageBoost</a> </h1>
                <h4>Conatct Us</h4>
            </div>
           
            <div class="form">
                <form method="post" action="/contact">
                   <div class="form-group">
                      <label for="exampleInputPassword1">Name</label>
                      <input name="Name" type="text" class="form-control" id="exampleName1" required>
                    </div>
                    <div class="form-group">
                      <label for="exampleInputEmail1">Email address</label>
                      <input name="Email" type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" required>
                      <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">subject</label>
                      <input name="subject" type="text" class="form-control" id="examplesubject" required>
                    </div>
                    <div class="form-group">
                      <label for="exampleInputPassword1">Message</label>
                      <input name="message" type="text" class="form-control" id="exampleMessage1" required>
                    </div>
                    
                    
                    <button type="submit" class="btn btn-primary">Submit</button>
                  </form>
            </div>
        </div>


